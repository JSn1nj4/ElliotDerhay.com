{
	"name": "jsn1nj4/elliotderhay.com",
	"description": "Personal website.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": "^8.4",
		"ext-pdo": "*",
		"coconutcraig/laravel-postmark": "^3.1",
		"filament/filament": "^3.2",
		"laravel/framework": "^12.14",
		"laravel/pennant": "^1.7",
		"laravel/tinker": "^2.9",
		"league/flysystem-aws-s3-v3": "^3.25",
		"league/flysystem-path-prefixing": "^3.10",
		"livewire/volt": "^1.7",
		"noweh/twitter-api-v2-php": "^3.4",
		"owenvoke/blade-fontawesome": "^2.9",
		"sentry/sentry-laravel": "^4.3",
		"spatie/laravel-markdown": "^2.5",
		"spatie/laravel-sitemap": "^7.0",
		"tempest/highlight": "^2.11"
	},
	"require-dev": {
		"barryvdh/laravel-ide-helper": "^3.0",
		"fakerphp/faker": "^1.23",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.0",
		"pestphp/pest": "^v3.7.1",
		"pestphp/pest-plugin-faker": "^3.0",
		"pestphp/pest-plugin-laravel": "^3.0",
		"pestphp/pest-plugin-livewire": "^3.0"
	},
	"autoload": {
		"files": [
			"bootstrap/helpers.php"
		],
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@php artisan filament:upgrade"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force",
			"@php artisan filament:upgrade"
		],
		"test": [
			"pnpm build",
			"@php artisan test",
			"pnpm build:clean"
		]
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true,
			"php-http/discovery": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
